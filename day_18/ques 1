// Write a program to print all factors of a given number.

#include <stdio.h>

int main() {
    int n, i;

    printf("Enter a number: ");
    scanf("%d", &n);

    printf("Factors of %d are: ", n);

    for (i = 1; i <= n; i++) {     // check all the factors 
        if (n % i == 0)            // remainder should be zero 
            printf("%d ", i);
    }

    printf("\n");
    return 0;
}
