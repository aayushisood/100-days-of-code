// Write a program to check if a number is prime

#include <stdio.h>

int main() {
    int n, i, count = 0;

    printf("Enter a number: ");
    scanf("%d", &n);

    if (n <= 1) { // prime num starts from 2
        printf("It is not a prime number.\n");
        return 0;
    }

    for (i = 1; i <= n; i++) {
        if (n % i == 0)
            count++;
    }

    if (count == 2) // 2 factors i.e. 1 and itself , if more than 2 cant be prime
        printf("It is a prime number.\n");
    else
        printf("It is not a prime number.\n");

    return 0;
}
